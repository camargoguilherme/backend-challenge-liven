{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommeded",
    "prettier/@typescript-eslint",
    "plugin:prettier/recomemended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": ["erros", "ignorePackages", { "ts": "never" }]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
